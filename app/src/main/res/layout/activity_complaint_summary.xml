<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_60sdp"
            android:orientation="vertical">
            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_marginTop="@dimen/_15sdp"
                android:layout_height="@dimen/_180sdp"
                android:layout_gravity="center"
                app:cardElevation="@dimen/_5sdp"
                app:cardCornerRadius="@dimen/_10sdp"
                android:layout_marginLeft="@dimen/_5sdp"
                android:layout_marginRight="@dimen/_5sdp"
                tools:ignore="MissingConstraints"
                android:visibility="visible"
                tools:layout_editor_absoluteX="2dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">


                    <View
                        android:layout_width="@dimen/_5sdp"
                        android:layout_height="match_parent"
                        android:layout_gravity="left"
                        android:background="@color/darkblue" />

                </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/_180sdp"
                android:layout_margin="@dimen/_5sdp"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_40sdp"
                    android:layout_gravity="center"
                    android:layout_margin="@dimen/_3sdp"
                    android:layout_marginTop="@dimen/_15sdp"

                    android:background="@drawable/rounded_button_edit"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_40sdp"
                        android:text="Cable Compalints: "
                        android:layout_weight="0.8"
                        android:gravity="center"
                        android:paddingStart="@dimen/_20sdp"
                        android:paddingEnd="@dimen/_20sdp"
                        android:fontFamily="@font/abeezee_regular"
                        android:drawableLeft="@drawable/ic_baseline_error_24"
                        android:textColor="@color/Black"
                        android:textSize="@dimen/_14sdp"></TextView>

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:text=" 96"
                        android:layout_weight="0.3"
                        android:paddingLeft="@dimen/_5sdp"
                        android:textColor="@color/Black"
                        android:textSize="@dimen/_14sdp"
                        android:textStyle="bold"></TextView>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="@dimen/_350sdp"
                        android:layout_height="@dimen/_100sdp"
                        android:background="@color/green"
                        android:layout_marginLeft="@dimen/_5sdp"
                        android:layout_marginTop="@dimen/_12sdp"
                        android:layout_weight="0.5"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="77"
                            android:gravity="center"
                            android:fontFamily="@font/abeezee_regular"
                            android:layout_marginTop="@dimen/_30sdp"
                            android:textColor="@color/White"
                            android:textSize="@dimen/_22sdp"></TextView>

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Closed"
                            android:gravity="center"
                            android:fontFamily="@font/abeezee_regular"
                            android:paddingLeft="@dimen/_5sdp"
                            android:textColor="@color/White"
                            android:textSize="@dimen/_14sdp"></TextView>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="@dimen/_350sdp"
                        android:layout_height="@dimen/_100sdp"
                        android:background="@color/red"
                        android:layout_marginLeft="@dimen/_5sdp"
                        android:layout_marginRight="@dimen/_5sdp"
                        android:layout_marginTop="@dimen/_12sdp"
                        android:layout_weight="0.5"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="12"
                            android:fontFamily="@font/abeezee_regular"
                            android:gravity="center"
                            android:layout_marginTop="@dimen/_30sdp"
                            android:textColor="@color/White"
                            android:textSize="@dimen/_22sdp"></TextView>

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Open"
                            android:gravity="center"
                            android:fontFamily="@font/abeezee_regular"
                            android:paddingLeft="@dimen/_5sdp"
                            android:textColor="@color/White"
                            android:textSize="@dimen/_14sdp"></TextView>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_marginTop="@dimen/_15sdp"
                android:layout_height="@dimen/_180sdp"
                android:layout_gravity="center"
                android:layout_marginBottom="@dimen/_30sdp"
                app:cardCornerRadius="@dimen/_10sdp"
                android:layout_marginLeft="@dimen/_5sdp"
                android:layout_marginRight="@dimen/_5sdp"
                app:cardElevation="@dimen/_5sdp"

                tools:ignore="MissingConstraints"
                android:visibility="visible"
                tools:layout_editor_absoluteX="2dp">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">


                    <View
                        android:layout_width="@dimen/_5sdp"
                        android:layout_height="match_parent"
                        android:layout_gravity="left"
                        android:background="@color/darkblue" />

                </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/_180sdp"
                android:layout_margin="@dimen/_5sdp"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_40sdp"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:layout_gravity="center"
                    android:layout_margin="@dimen/_3sdp"
                    android:background="@drawable/rounded_button_edit"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Internet Compalints:"
                        android:layout_weight="0.1"
                        android:gravity="center"
                        android:paddingLeft="@dimen/_15sdp"
                        android:fontFamily="@font/abeezee_regular"
                        android:drawableLeft="@drawable/ic_baseline_error_24"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:textColor="@color/Black"
                        android:textSize="@dimen/_14sdp"></TextView>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text=" 76"
                        android:layout_weight="0.3"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:paddingLeft="@dimen/_5sdp"
                        android:textColor="@color/Black"
                        android:textSize="@dimen/_14sdp"
                        android:textStyle="bold"></TextView>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_15sdp"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="@dimen/_350sdp"
                        android:layout_height="@dimen/_100sdp"
                        android:background="@color/green"
                        android:layout_marginLeft="@dimen/_5sdp"
                        android:layout_weight="0.5"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="37"
                            android:gravity="center"
                            android:fontFamily="@font/abeezee_regular"
                            android:layout_marginTop="@dimen/_30sdp"
                            android:textColor="@color/White"
                            android:textSize="@dimen/_22sdp"></TextView>

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Closed"
                            android:gravity="center"
                            android:fontFamily="@font/abeezee_regular"
                            android:paddingLeft="@dimen/_5sdp"
                            android:textColor="@color/White"
                            android:textSize="@dimen/_14sdp"></TextView>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="@dimen/_350sdp"
                        android:layout_height="@dimen/_100sdp"
                        android:background="@color/red"
                        android:layout_marginLeft="@dimen/_5sdp"
                        android:layout_marginRight="@dimen/_5sdp"
                        android:layout_weight="0.5"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="17"
                            android:gravity="center"
                            android:fontFamily="@font/abeezee_regular"
                            android:layout_marginTop="@dimen/_30sdp"
                            android:textColor="@color/White"
                            android:textSize="@dimen/_22sdp"></TextView>

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Open"
                            android:fontFamily="@font/abeezee_regular"
                            android:gravity="center"
                            android:paddingLeft="@dimen/_5sdp"
                            android:textColor="@color/White"
                            android:textSize="@dimen/_14sdp"></TextView>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
                </LinearLayout>
            </androidx.cardview.widget.CardView>
        </LinearLayout>
    </ScrollView>
</LinearLayout>